{
  "manifest_version": 2,
  "name": "Multi Homepage",
  "description": "Open multiple urls with a single click.",
  "version": "1.0.0",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Multi Homepage",
    "default_icon": "icon-256.png"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
  }
}
