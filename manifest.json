{
  "manifest_version": 3,
  "name": "Multi Homepage",
  "description": "Open multiple URLs with a single click.",
  "version": "2.0.0",
  "action": {
    "default_title": "Multi Homepage",
    "default_icon": "./resources/icon/icon-256.png"
  },
  "background": {
    "service_worker": "./assets/background.js"
  },
  "options_page": "./src/options/index.html",
  "icons": {
    "256": "./resources/icon/icon-256.png"
  },
  "permissions": [
    "storage"
  ]
}
